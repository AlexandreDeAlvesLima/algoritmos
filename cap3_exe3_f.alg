algoritmo "semnome"
// Função :
// Autor :
// Data : 16/4/2018
// Seção de Declarações 
var
   a,b,x: inteiro
inicio
// Seção de Comandos
   escreva("Informe um número: ")
   leia(a)
   escreva("Informe outro valor: ")
   leia(b)
   x <- a
   a <- b
   b <- x
   escreval("O valor de A: ", a)
   escreval("O valor de B: ",b)
fimalgoritmo
